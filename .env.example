# Backend
PORT=8080
ADMIN_PASSWORD=admin123

# Supabase (Backend gunakan SERVICE_ROLE untuk operasi server-side)
SUPABASE_URL=https://YOUR-PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Supabase (Frontend gunakan ANON KEY)
VITE_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Email (Nodemailer SMTP)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=username
SMTP_PASS=password
NOTIFY_EMAILS=admin1@example.com,admin2@example.com

# Feature Flags
PUBLIC_CALENDAR_VISIBLE=true
AI_SUGGESTION_PROVIDER=gemini
